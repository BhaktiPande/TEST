    <script>
        function deletecookie() {
            try {
                var pairs = document.cookie.split(";");
                var cookies = [];
                for (var i = 0; i < pairs.length; i++) {
                    var pair = pairs[i].split("=");
                    cookies.push(pair[0]);
                }
                $.each(cookies, function (index, value) {
                    $.cookie(value, null, { expires: -1, path: '/' });
                });
            } catch (e) { }
        }
        try {
            var pairs = document.cookie.split(";");
            for (var i = 0; i < pairs.length; i++) {
                deletecookie();
            }
        } catch (e) { }
    </script>
