@Html.Hidden("GetTradingWindow", Url.Action("GetTradingWindow", "TradingWindowsOther"))
@Html.Hidden("GetTradingWindowCalendar", Url.Action("GetTradingWindowCalendar", "TradingWindowsOther"))

<script type="text/javascript">
    $(document).ready(function () {
        var a= "@InsiderTrading.Common.ConstEnum.UserActions.TRADING_WINDOW_OTHER_VIEW";

        if ('@InsiderTrading.Common.Common.CanPerform('a')') {
            getTradingWindow();
         }


        $(document).delegate('#prev', 'click', function (event) {
            var Year = $('#hidCurrentyear').val();
            var Month = $('#hidCurrentMonth').val();
            $('#divTradingWindow').html("");
            $.ajax({
                url: $("#GetTradingWindowCalendar").val(),
                type: 'post',
                headers: getRVToken(),
                cache: false,
                data: { "Year": Year, "sMonth": Month, "CalledFrom": "Prev" },
                success: function (result) {
                    $('#divTradingWindow').html(result);
                },
                error: function (result) {
                }

            });
        });

        $(document).delegate('#next', 'click', function (event) {
            var Year = $('#hidCurrentyear').val();
            var Month = $('#hidCurrentMonth').val();
            $('#divTradingWindow').html("");
            $.ajax({
                url: $("#GetTradingWindowCalendar").val(),
                type: 'post',
                headers: getRVToken(),
                cache: false,
                data: { "Year": Year, "sMonth": Month, "CalledFrom": "Next" },
                success: function (result) {
                    $('#divTradingWindow').html(result);
                },
                error: function (result) {
                }

            });
        });
    });

    function getTradingWindow() {
        $.ajax({
            url: $("#GetTradingWindow").val(),
            type: 'post',
            headers: getRVToken(),
            data: {},
            success: function (data) {
                $("#divTradingWindow").html(data);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                alert(errorThrown);
            }
        });
    }
</script>


