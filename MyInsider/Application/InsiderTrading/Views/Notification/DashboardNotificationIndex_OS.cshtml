
<div class="box box-chart">
    <div class="box-header with-border">
        <h3 class="box-title"><i class="fa fa-envelope"></i>@InsiderTrading.Common.Common.getResource("dis_lbl_54204")</h3>
        <a href='@Url.Action("Index", "Notification", new { acid = InsiderTrading.Common.ConstEnum.UserActions.NOTIFICATION_LIST_RIGHT ,CalledFrom="OS"})'><button type="button" class="btn btn-sm btn-success pull-right">@InsiderTrading.Common.Common.getResource("dis_btn_54202")</button></a>
    </div>
    <div class="box-body no-padding">
        <div class="table-responsive mailbox-condensed">
            <table class="table table-hover table-striped">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Subject</th>
                    </tr>
                </thead>
            </table>
            <div id="mail-list">
                <table class="table table-hover table-striped">
                    <tbody>
                        @if (!ViewBag.NoRecord_OS)
                        {
                            var CMU = 0;
                            foreach (var objNotificationList in ViewBag.NotificationDictionary_OS)
                            {
                                if (ViewBag.RequiredModuleBoth)
                                {
                                    if (objNotificationList.NotificationTYPE == "CMU")
                                    {
                                        CMU = 1;
                                        <tr>
                                            <td>@InsiderTrading.Common.Common.ApplyFormatting(@objNotificationList.CreatedOn, InsiderTrading.Common.ConstEnum.DataFormatType.Date)</td>
                                            <td>
                                                <a href='@Url.Action("PendingCount", "CODashboard", new { acid = 185, DashboardId = (@objNotificationList.NotificationTYPE=="CMU")? 19:25, count = 0, NotificationQueueId = @objNotificationList.NotificationQueueId, inp_sParam = 0 })'>
                                                    @objNotificationList.Contents
                                                </a>
                                            </td>
                                        </tr>
                                    }


                                }
                                else
                                {
                                    CMU = 1;
                                    <tr>
                                        <td>@InsiderTrading.Common.Common.ApplyFormatting(@objNotificationList.CreatedOn, InsiderTrading.Common.ConstEnum.DataFormatType.Date)</td>
                                        <td>
                                            <a href='@Url.Action("PendingCount", "CODashboard", new { acid = 185, DashboardId = (@objNotificationList.NotificationTYPE=="CMU")? 19:25, count = 0, NotificationQueueId = @objNotificationList.NotificationQueueId, inp_sParam = 0 })'>
                                                @objNotificationList.Contents
                                            </a>
                                        </td>
                                    </tr>
                                }

                            }
                            if (CMU == 0)
                            {

                                <tr>
                                    <td>No records found</td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr>
                                <td>No records found</td>
                            </tr>
                        }

                    </tbody>
                </table><!-- /.table -->
            </div>
        </div><!-- /.mail-box-messages -->
    </div><!-- /.box-body -->
</div>
