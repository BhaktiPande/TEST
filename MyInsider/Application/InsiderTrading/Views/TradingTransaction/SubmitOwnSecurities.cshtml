
<section class="content-header">

    <div class="row">
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1" style="width:3% !important">
            @Html.CheckBox("Chk_Details", new { @Name = "IsChecked", @value = "true" })
        </div>
        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11" style="padding-bottom:20px; width:97% !important">
            <p>
                @InsiderTrading.Common.Common.getResource("dis_lbl_52083")
            </p>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <button type="button" class="btn btn-success" id="ConfirmDetails" data-toggle="modal"> @InsiderTrading.Common.Common.getResource("dis_btn_52084")</button>
        </div>
    </div>

</section>

<script type="text/javascript">
    $(document).delegate("#ConfirmDetails", "click", function () {
        if ($('#Chk_Details').prop("checked") == false) {
            showMessage('@InsiderTrading.Common.Common.getResource("dis_msg_52085")', false);
        }
        else
        {
            $("#btnInsiderSubmit").trigger("click");
        }
    })
</script>



