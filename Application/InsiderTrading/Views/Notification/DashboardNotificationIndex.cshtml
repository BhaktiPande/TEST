
<div class="box box-chart">
    <div class="box-header with-border">
        <h3 class="box-title"><i class="fa fa-envelope"></i> @InsiderTrading.Common.Common.getResource("cmu_lbl_18060")</h3>
        <a href='@Url.Action("Index", "Notification", new { acid = InsiderTrading.Common.ConstEnum.UserActions.NOTIFICATION_LIST_RIGHT })'><button type="button" class="btn btn-sm btn-success pull-right">@InsiderTrading.Common.Common.getResource("cmu_btn_18048")</button></a>
    </div>
    <div class="box-body no-padding">
        <div class="table-responsive mailbox-condensed">
            <table class="table table-hover table-striped">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Subject</th>
                    </tr>
                </thead>
            </table>
            <div id="mail-list">
                <table class="table table-hover table-striped">
                    <tbody>
                        @if (!ViewBag.NoRecord)
                        {
                            foreach (var objNotificationList in ViewBag.NotificationDictionary)
                            {
                                <tr>
                                    <td>@InsiderTrading.Common.Common.ApplyFormatting(@objNotificationList.CreatedOn, InsiderTrading.Common.ConstEnum.DataFormatType.Date)</td>
                                    <td>
                                        <a href='@Url.Action("PendingCount", "CODashboard", new { acid = 185, DashboardId =(@objNotificationList.NotificationTYPE=="CMU")? 19:25, count = 0, NotificationQueueId = @objNotificationList.NotificationQueueId, inp_sParam = 0})'>
                                            @objNotificationList.Contents
                                        </a>
                                    </td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr>
                                <td>No records found</td>
                            </tr>
                        }

                    </tbody>
                </table><!-- /.table -->
            </div>
        </div><!-- /.mail-box-messages -->
    </div><!-- /.box-body -->
</div>
